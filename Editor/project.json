{
    "version_framework": "1.0",
    "version": "1.0",
    "name": "ParticuleEditor",
    "description": "Description du projet",
    "author": "Auteur du projet",
    "date": "02/08/2024",
    "debug": false,
    "flags": "-Wall -Wextra -Werror",
    "sources": [
        "srcs/ParticuleHub/ParticuleHub.cpp"
    ],
    "includes": [
        "includes"
    ],
    "output": "bin",
    "build": "build",
    "packages": [
        "InterfaceSystem"
    ],
    "distributions": {
        "Windows": {
            "debug-flags": "",
            "executable": "ParticuleEditor.exe",
            "resources": {
                "ico": "assets/WinLinux/icon.ico",
                "icon": "assets/WinLinux/icon.png",
                "asset_dir": "assets",
                "images": []
            }
        },
        "Linux": {
            "debug-flags": "-fsanitize=address -g3 -std=c99 -O0",
            "executable": "ParticuleEditor",
            "resources": {
                "icon": "assets/WinLinux/icon.png",
                "asset_dir": "assets",
                "images": []
            }
        },
        "CasioCg": {
            "debug-flags": "",
            "executable": "Editor.g3a",
            "display_name": "Editor",
            "resources": {
                "icon-uns": "assets/CasioCg/icon-uns.png",
                "icon-sel": "assets/CasioCg/icon-sel.png",
                "images": []
            }
        }
    }
}